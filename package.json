{
  "name": "suan-mai",
  "version": "1.0.0",
  "private": true,
  "description": "Suan Mai is a garden for your data, configurations, and schemas. Design, grow, and nurture structured information like plants in a well-tended garden. Manage your data, maintain reusable structures, and publish APIs for other applications effortlessly.",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://patchamet@github.com/patchamet/suan-mai.git"
  },
  "keywords": ["turborepo", "nextjs", "nestjs", "graphql", "prisma", "microservices"],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/patchamet/suan-mai/issues"
  },
  "homepage": "https://github.com/patchamet/suan-mai#readme",
  "devDependencies": {
    "turbo": "^1.10.12",
    "@types/node": "^20.5.0",
    "typescript": "^5.1.6"
  },
  "packageManager": "npm@9.8.1",
  "engines": {
    "node": ">=18.0.0"
  }
}
