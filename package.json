{
  "name": "suan-mai",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:auth": "turbo run dev --filter=service-auth",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "-----docker section-----": "-------------------------------- DOCKER SECTION --------------------------------",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:restart": "turbo run docker:down && turbo run docker:up",
    "docker:logs": "docker compose logs -f"

  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
